package com.patterns.command.second;

public class WingsControls implements CommandController {
	private Wings wings;
	public final String MOVE_UP = "MOVE_UP";
	public final String SLOW_DOWN = "SLOW_DOWN";
	
	public WingsControls(){
		wings = new Wings();
	}
	
	@Override
	public String execute(String command) {
		switch(command) {
		case MOVE_UP :
			wings.moveUp();
			break;
		case SLOW_DOWN :
			wings.moveDown();
		}
		return null;
	}

	@Override
	public boolean canHandle(String command) {
		// TODO Auto-generated method stub
		return false;
	}

}
